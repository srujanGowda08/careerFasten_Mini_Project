    <title>CareerFasten</title>
  </head>
  <body>
    <!DOCTYPE html>
    <html lang="en">
      <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />

        <!--========== BOX ICONS ==========-->
        <link
          rel="stylesheet"
          href="https://cdn.jsdelivr.net/npm/boxicons@latest/css/boxicons.min.css"
        />
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
        
        <!--========== CSS ==========-->
        <link rel="stylesheet" href="/css/home.css" />
        <link rel="stylesheet" href="/css/roadmap.css">
        <script src="https://cdn.tailwindcss.com"></script>
      </head>
      <body>
        <!--========== HEADER ==========-->
        <header class="header">
          <div class="header__container">
            <div class="header__search">
              <input type="text" value="<%=user.usn.toUpperCase()%>" class="header__input user " readonly />
            </div>
            

            <a href="/home" class="header__logo">CareerFasten</a>

            <div class="header__search">
              <input type="button" value="PlayGround" class="header__input " />
            
            </div>

            <div class="header__toggle">
              <i class="bx bx-menu" id="header-toggle"></i>
            </div>
          </div>
        </header>

        <!--========== NAV ==========-->
        <div class="nav" id="navbar">
          <nav class="nav__container">
            <div>
              <a href="/home" class="nav__link nav__logo">
                <i class="bx bxs-disc nav__icon"></i>
                <span class="nav__logo-name">CareerFasten</span>
              </a>

              <div class="nav__list">
                <div class="nav__items">
                  <a href="/home" class="nav__link active">
                    <i class="bx bx-home nav__icon"></i>
                    <span class="nav__name">Home</span>
                  </a>
                  <a href="/roadmap" class="nav__link">
                    <i class='bx bx-map nav__icon'></i>
                    <span class="nav__name">RoadMap</span>
                  </a>

                

                  <a href="/upcoming" class="nav__link">
                    <i class='bx bx-repost nav__icon' ></i>
                    <span class="nav__name">Upcoming_Updates</span>
                  </a>
                  <a href="/resources" class="nav__link">
                    <i class='bx bx-note nav__icon'></i>
                    <span class="nav__name">Resources</span>
                  </a>
                  <a href="/resumebuilder" class="nav__link">
                    <i class='bx bxs-book-content nav__icon'></i>
                    <span class="nav__name">Resume_Builder</span>
                  </a>

                  <a href="/discuss" class="nav__link">
                    <i class="bx bx-message-rounded nav__icon"></i>

                    <span class="nav__name">Discuss</span>
                  </a>
               
            <a href="/logout" class="nav__link nav__logout">
              <i class="bx bx-log-out nav__icon"></i>
              <span class="nav__name">Log Out</span>
            </a>
          </nav>
        </div>

        <!--========== CONTENTS ==========-->
        <main>
          <div class="container mx-auto px-4 mt-10">
            <div class="text-center mb-10">
                <h1 class="text-4xl font-bold mt-20">Campus Placements Prep for Beginners</h1>
            </div>
            <div class="flex flex-col items-center">
                <!-- Understanding the Process -->
                <div class="flex items-center w-full mb-6">
                    <div class="w-1/2 text-right pr-6">
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Awareness About Campus Placement Process</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Pre-Placement Talk</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Written Test</div>
                    </div>
                    <div class="w-1/12 text-center">
                        <div class="bg-green-500 text-white p-2 rounded-md shadow-md inline-block">Understanding the Process</div>
                    </div>
                    <div class="w-1/2 pl-6">
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Group Discussion</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Technical Interview</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">HR Interview</div>
                    </div>
                </div>
    
                <!-- Skill Assessment -->
                <div class="flex items-center w-full mb-6">
                    <div class="w-1/2 text-right pr-6">
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Identifying Strengths and Weaknesses</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Technical Skills Needed</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Soft Skills Development</div>
                    </div>
                    <div class="w-1/12 text-center">
                        <div class="bg-green-500 text-white p-2 rounded-md shadow-md inline-block">Skill Assessment</div>
                    </div>
                    <div class="w-1/2 pl-6">
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Aptitude Skills</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Leadership Qualities</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Communication Skills</div>
                    </div>
                </div>
    
                <!-- Resume Building -->
                <div class="flex items-center w-full mb-6">
                    <div class="w-1/2 text-right pr-6">
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Basics of Resume Writing</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Layout and Formatting</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Highlighting Relevant Skills and Experiences</div>
                    </div>
                    <div class="w-1/12 text-center">
                        <div class="bg-green-500 text-white p-2 rounded-md shadow-md inline-block">Resume Building</div>
                    </div>
                    <div class="w-1/2 pl-6">
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Resume Review and Feedback</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Cover Letter Writing</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Online Resume Builders and LinkedIn Profile</div>
                    </div>
                </div>
    
                <!-- Aptitude Tests -->
                <div class="flex items-center w-full mb-6">
                    <div class="w-1/2 text-right pr-6">
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Quantitative Aptitude</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Logical Reasoning</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Verbal Ability</div>
                    </div>
                    <div class="w-1/12 text-center">
                        <div class="bg-green-500 text-white p-2 rounded-md shadow-md inline-block">Aptitude Tests</div>
                    </div>
                    <div class="w-1/2 pl-6">
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Data Interpretation</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Practice Tests</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Speed and Accuracy Building</div>
                    </div>
                </div>
    
                <!-- Technical Knowledge -->
                <div class="flex items-center w-full mb-6">
                    <div class="w-1/2 text-right pr-6">
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Basics of Computer Science</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Programming Languages</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Data Structures and Algorithms</div>
                    </div>
                    <div class="w-1/12 text-center">
                        <div class="bg-green-500 text-white p-2 rounded-md shadow-md inline-block">Technical Knowledge</div>
                    </div>
                    <div class="w-1/2 pl-6">
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Software Development Principles</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Database Management</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Networking Basics</div>
                    </div>
                </div>
    
                <!-- Group Discussions -->
                <div class="flex items-center w-full mb-6">
                    <div class="w-1/2 text-right pr-6">
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Understanding Group Discussion Basics</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Topic Awareness and Current Affairs</div>
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Communication Skills</div>
                    </div>
                    <div class="w-1/12 text-center">
                        <div class="bg-green-500 text-white p-2 rounded-md shadow-md inline-block">Group Discussions</div>
                    </div>
                    <div class="w-1/2 pl-6">
                        <div class="bg-yellow-200 p-4 rounded-md shadow-md mb-4 inline-block">Leadership Skills</div>
